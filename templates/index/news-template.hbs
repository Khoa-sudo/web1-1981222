<h1>News</h1>
<div class="row">
  {{#each data}}
    <div class="col-lg-4 col-md-12 col-sm-12">
      <div class="float-start me-3">
        <div class="date text-white text-center rounded-4">
          <h3 class="mb-0">
            {{date}}
          </h3>
        </div>
        <a href="?request=news/{{id}}" class="btn btn-link ps-0">Read more</a>
      </div>
      <div>
        <h3>{{name}}</h3>
        <p>{{summary}}</p>
      </div>
    </div>
  {{/each}}
</div>