<h1 class="py-3">Our Vision</h1>
{{#each data}}
  <div class="clearfix">
    <a
      class="text-decoration-none"
      href="#visions/{{id}}"
      onclick="loadDetails('visions/{{id}}','details-template','content')"
    >
      <img
        src="{{thumbpath}}"
        alt="{{name}}"
        class="float-start rounded-4 me-3"
      />
      <h3 class="text-black">{{name}}</h3>
      <p>{{summary}}</p>
    </a>
  </div>
{{/each}}